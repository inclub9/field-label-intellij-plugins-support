<?xml version="1.0" encoding="UTF-8"?>
<idea-plugin>
    <id>inclub9.fieldlabel</id>
    <name>FieldLabel Support</name>
    <vendor>inclub9</vendor>
    <description>Provides IDE support for @FieldLabel annotation</description>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <lang.psiAugmentProvider implementation="inclub9.intellij.plugin.fieldlabel.FieldLabelContributor"/>
        <localInspection language="JAVA"
                         displayName="FieldLabel Inspection"
                         groupName="FieldLabel"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="inclub9.intellij.plugin.fieldlabel.inspection.FieldLabelInspection"/>
    </extensions>
</idea-plugin>